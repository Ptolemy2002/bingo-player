{
  "name": "@ptolemy2002/bingo-player-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bash ./scripts/build.sh && tsx ./bin/www.ts",
    "dev": "bash ./scripts/build.sh && tsx watch ./bin/www.ts --exclude swagger_output.json",
    "build": "pnpm swagger-autogen",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx typesync",
    "link-shared": "bash ./scripts/link-shared.sh",
    "uninstall": "bash ./scripts/uninstall.sh",
    "reinstall": "bash ./scripts/reinstall.sh",
    "swagger-autogen": "tsx src/services/swagger.ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ptolemy2002/bingo-player",
    "directory": "api"
  },
  "dependencies": {
    "@ptolemy2002/express-utils": "^2.1.1",
    "@ptolemy2002/js-utils": "^3.2.2",
    "@ptolemy2002/mongoose-utils": "^1.1.1",
    "@ptolemy2002/regex-utils": "^4.2.0",
    "@ptolemy2002/ts-utils": "^3.4.0",
    "@types/is-callable": "^1.1.2",
    "app-module-path": "^2.2.0",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.6",
    "debug": "~2.6.9",
    "dotenv": "^16.6.1",
    "escape-html": "^1.0.3",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "http-errors": "^2.0.1",
    "is-callable": "^1.2.7",
    "mongoose": "^8.21.1",
    "morgan": "~1.9.1",
    "ms": "^2.1.3",
    "shared": "link:../shared",
    "socket.io": "^4.8.3",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/app-module-path": "~2.2.2",
    "@types/cookie-parser": "~1.4.10",
    "@types/cors": "~2.8.19",
    "@types/debug": "~4.1.12",
    "@types/escape-html": "~1.0.4",
    "@types/express": "~5.0.6",
    "@types/http-errors": "~2.0.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "~1.9.10",
    "@types/ms": "~0.7.34",
    "@types/node": "^22.19.7",
    "@types/swagger-ui-express": "~4.1.8",
    "http": "0.0.1-security",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3"
  }
}
