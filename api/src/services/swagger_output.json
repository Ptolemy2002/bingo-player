    "/api/v1/spaces/get/all/list/{prop}": {
      "get": {
        "tags": [
          "Spaces"
        ],
        "description": "Get all values for a given space query prop.",
        "parameters": [
          {
            "name": "prop",
            "in": "path",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/SpaceQueryProp"
            },
            "description": "The space query prop to list.",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "List of values found",
            "content": {
              "application/json": {
                "schema": {
                  "$ok": true,
                  "$values": [
                    "string"
                  ],
                  "help": "https://example.com/docs"
                },
                "example": {
                  "ok": true,
                  "values": [
                    "value1",
                    "value2"
                  ],
                  "help": "https://example.com/docs"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
